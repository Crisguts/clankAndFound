    "express": "^5.2.1",
    "multer": "^2.0.2",
    "node": "^25.3.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.8.0",
    "next": "latest"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
